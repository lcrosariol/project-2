

<%- include('../partials/header') %>

<div>Please use airport ICAO identifier and ensure that it is not in the following list before adding:</div>
<section id="badge-container">
  <% performers.forEach(function(p) { %>
    <article class="badge"><%= p.name %></article>
  <% }) %>
</section>

<form id="add-performer-form" action="/performers" method="POST">
  <label>ENROUTE/ALTERNATE:</label>
  <input type="text" name="name">
  <label>ETA (Local Time Please):</label>
  <input type="time" name="born">
  <input type="submit" value="Add Enroute/Alternate">
</form>

<%- include('../partials/footer') %>


